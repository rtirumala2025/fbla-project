[run]
source = .
omit =
    */tests/*
    */__pycache__/*
    */migrations/*
    */alembic/*
    */node_modules/*
    */venv/*
    */env/*
    frontend/*
    e2e/*
    scripts/*
    docs/*
    reports/*

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

